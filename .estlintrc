{
  "parser": "babel-eslint",
  "env": {
  "browser": true,
  "node": true
  },
  "rules": {
    "strict":0
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": false
  },
  "plugins": [
    "react"
  ]
}
